{
  "name": "superterminal",
  "version": "0.1.0",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "pack:win": "electron-packager . SuperTerminal --platform=win32 --arch=x64 --out=dist --overwrite --asar --prune=true --ignore=^dist$ --ignore='node_modules/.*/(README|Readme|readme)(\\..*)?$' --ignore='node_modules/.*/(test|tests|__tests__|example|examples)/' --ignore='.*\\.(pem|key|pub)$'"
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "ssh2": "^1.15.0",
    "ssh2-sftp-client": "^9.0.5",
    "@xterm/xterm": "^5.3.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-clipboard": "^0.2.0"
  }
}
